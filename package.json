	    "@rollup/plugin-typescript": "^6.0.0",
	    "@tsconfig/svelte": "1.0.10",
		"@types/node": "^16.11.6",
		"@typescript-eslint/eslint-plugin": "^5.2.0",
		"@typescript-eslint/parser": "^5.2.0",
		"builtin-modules": "^3.2.0",
		"esbuild": "0.13.12",
		"obsidian": "^0.12.17",
		"rollup": "^2.51.2",
	    "rollup-plugin-svelte": "7.0.0",
		"svelte": "^3.38.2",
	    "svelte-preprocess": "4.6.1",
		"tslib": "2.3.1",
		"typescript": "4.4.4"
