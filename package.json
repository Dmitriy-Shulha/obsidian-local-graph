{
	"name": "obsidian-local-graph",
	"version": "0.0.1",
	"description": "This plugin is a custom implementation of Local Graph View for Obsidian.md.",
	"main": "main.js",
	"scripts": {
		"dev": "rollup --config rollup.config.js -w",
		"build": "rollup --config rollup.config.js",
		"dev_es": "node esbuild.config.mjs",
		"build_es": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
		"version": "node version-bump.mjs && git add manifest.json versions.json"
	},
	"keywords": [],
	"author": "",
	"license": "MIT",
	"devDependencies": {
		"@rollup/plugin-commonjs": "^15.1.0",
    	"@rollup/plugin-json": "^4.1.0",
